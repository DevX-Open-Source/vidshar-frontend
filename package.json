    "prettier": "^2.8.0",
    "storybook": "^6.5.13",
    "storybook-addon-next-router": "^4.0.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.9.3",
    "webpack": "5.68.0"
  }
